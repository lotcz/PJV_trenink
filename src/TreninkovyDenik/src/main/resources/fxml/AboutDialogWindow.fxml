            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label text="pro cvičení s kettlebellem" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/images/kettlebell1.jpg" />
         </image>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Karel Zavadil" VBox.vgrow="NEVER">
