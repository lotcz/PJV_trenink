<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.zavadil.treninkovydenik.AboutDialogWindowController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Tréninkový deník" textAlignment="CENTER" VBox.vgrow="ALWAYS">
